package com.jicheng.chunwan;
/**
 * 演员父类
 * @author 87366
 *
 */
public class Actor {
	private String name;
	
	public Actor() {
		
	}
	
	public Actor(String name) {
		setName(name);
	}
	/**
	 * 演员的表演方法
	 * @return
	 */
	public void performs() {
		System.out.println("演员" + name + "正在表演精彩的节目");
	}
	
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}
	
}
