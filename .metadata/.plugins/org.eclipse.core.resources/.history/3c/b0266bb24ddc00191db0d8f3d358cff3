
public class blogTest {
	public static void main(String[] args) {
		int[] arr = {52,63,14,59,68,35,8,67,45,99};
		System.out.println("原数组：");
		for (int i : arr) {
			System.out.print(i+" ");
		}
		System.out.println();
		for(int i = 0; i < arr.length-1; i++){
			int min = i;
			for(int j = i+1; j <arr.length-1;j++){
				if(arr[j]<arr[min]){
					min = j;
				}
			}
			if(min!=i){
				int temp = arr[i];
				arr[i] = arr[min];
				arr[min] = temp;
			}
		}
		System.out.println("排序后：");
		for (int i : arr) {
			System.out.print(i+" ");
		}
	}

}

