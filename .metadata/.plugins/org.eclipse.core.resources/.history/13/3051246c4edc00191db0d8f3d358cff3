import java.util.Arrays;
public class blogTest {
	public static void main(String[] args) {
		int[] arr = {52,63,14,59,68,35,8,67,45,99};
		System.out.println("原数组：");
		for (int i : arr) {
			System.out.print(i+" ");
		}
		System.out.println();
		Arrays.sort(arr);
		System.out.println("排序后：");
		for (int i : arr) {
			System.out.print(i+" ");
		}
	}
}

 

 

 

 

 







